{
    "name": "photo-library-server",
    "version": "0.0.0",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "watch:build": "tsc --watch",
        "watch:server": "nodemon './dist/index.js' --watch './dist'",
        "start": "npm-run-all clean build --parallel watch:build watch:server --print-label",
        "test": "jest --env=node --colors --coverage test",
        "circular": "madge src/app.ts -c",
        "madge": "madge src/app.ts -i madge.png; open madge.png",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install --legacy-peer-deps"
    },
    "devDependencies": {
      "@faker-js/faker": "^7.2.0",
      "@types/better-sqlite3": "^7.6.2",
      "@types/chai": "^4.3.1",
      "@types/crypto-js": "^4.1.1",
      "@types/debug": "^4.1.6",
      "@types/express": "^4.17.13",
      "@types/express-fileupload": "^1.2.2",
      "@types/express-session": "^1.17.4",
      "@types/image-thumbnail": "^1.0.1",
      "@types/jest": "^27.5.0",
      "@types/minio": "^7.0.13",
      "@types/morgan": "^1.9.3",
      "@types/node": "^17.0.31",
      "@types/sharp": "^0.31.0",
      "@types/sqlite3": "^3.1.8",
      "@types/supertest": "^2.0.12",
      "@types/swagger-jsdoc": "^6.0.1",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/tesseract.js": "^2.0.0",
      "chai": "^4.3.6",
      "chai-http": "^4.3.0",
      "gitinspector": "^0.5.0-dev-2",
      "http-request-mock": "^1.7.6",
      "jest": "^28.1.0",
      "jest-extended": "^1.1.0",
      "madge": "^5.0.1",
      "nodemon": "^2.0.16",
      "npm-run-all": "^4.1.5",
      "shx": "^0.3.4",
      "supertest": "^6.2.3",
      "ts-jest": "^28.0.2",
      "ts-node": "^10.7.0",
      "typescript": "^4.6.4"
    },
    "dependencies": {
      "@elastic/elasticsearch": "^8.7.0",
      "better-sqlite3": "^7.6.2",
      "cors": "^2.8.5",
      "crypto-js": "^4.1.1",
      "debug": "^4.3.4",
      "dotenv": "^16.0.1",
      "express": "^4.18.1",
      "minio": "^7.1.0",
      "morgan": "^1.10.0",
      "pino": "^8.0.0",
      "pino-http": "^8.0.1",
      "pino-pretty": "^8.0.0",
      "sharp": "^0.31.1",
      "swagger-jsdoc": "^6.2.1",
      "swagger-ui-express": "^4.5.0",
      "tesseract.js": "^2.1.1",
      "yup": "^0.32.11",
      "source-map-support": "^0.5.21"
    }
}
